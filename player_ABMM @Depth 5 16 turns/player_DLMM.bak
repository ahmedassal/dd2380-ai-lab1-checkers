int Player::calculateNumberOfCheckers(std::string board)
{
	int counter = 0;
	for (int i = 0; i < board.length(); i++)
	{
		if(board[i] == 'r' || board[i] == 'R' || board[i] == 'w' || board[i] == 'W')
			counter++;
	}
	return counter;
}

int Player::calcPosValue(int pos, int color)
{
	int value;

	if(color == CELL_WHITE)
	{
		if (pos >= 4 && pos <=7)
			value = 7;
		else
			value = 5;
	}else
	{
		if (pos >= 24 && pos <= 27)
			value = 7;
		else
			value = 5;
	}
	return value * tablePosValue[pos];
}



